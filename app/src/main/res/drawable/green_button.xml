<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#65ffe5" />
            <stroke
                android:width="1dp"
                android:color="#65ffe5" />
            <corners
                android:radius="10dp" />

        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="@color/colorSecondary"
                ></solid>

            <stroke

                android:color="@color/colorSecondary" />
            <corners
                android:radius="10dp" />

        </shape>
    </item>

</selector>